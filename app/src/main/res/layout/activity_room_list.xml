<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.RoomListActivity">
    <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_room_list"
            android:layout_width="match_parent"
            android:layout_height="?actionBarSize"
            android:background="@color/colorPrimary">
        <com.caltyfarm.caltyfarm.customElements.CustomTextView
                android:id="@+id/tv_title_room_list"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/daftar_dokter_hewan"
                android:textColor="@color/white"
                android:textSize="18sp"/>
    </androidx.appcompat.widget.Toolbar>
    <com.qiscus.sdk.ui.view.QiscusRecyclerView
            android:id="@+id/qs_rv_room_list"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/toolbar_room_list"
            app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>